    "devDependencies": {
        "tslib": "2.3.0",
        "chokidar": "3.5.3",
        "typescript": "5.6.2",
        "@types/node": "22.6.1",
        "@types/inquirer": "9.0.7",
        "@swc/cli": "0.1.57",
        "@swc/core": "1.2.171",
        "@x-drive/changelog": "1.1.6"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/x-9lab/launch.git"
    },
